2 ?{<let>}
3 ?{repeatatleast(<cap>,7)}
4 ?{repeatatleast(<let>,7)}
5 ?{contain(<cap>)}
6 ?{contain(<let>)}
7 ?{repeatatleast(contain(<cap>),7)}
8 ?{repeatatleast(contain(<let>),7)}
